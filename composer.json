{
    "name": "nhacsam/datatables-bundle",
    "type": "symfony-bundle",
    "description": "Symfony DatatableBundle",
    "keywords": ["Datatable", "Table"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Djambazian",
            "email": "nicolas@djambazian.fr",
            "homepage": "http://nicolas.djambazian.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "~2.3"
    },
    "autoload": {
        "psr-0": { "Nhacsam\\DataTablesBundle\\": "" }
    },
    "target-dir": "Nhacsam/DataTablesBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.0-dev"
        }
    }
}

